{
  "version": 2,
  "name": "capstone-backend",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/get-appointment",
      "dest": "/api/get-appointment.js"
    },
    {
      "src": "/api/login",
      "dest": "/api/login.js"
    },
    {
      "src": "/api/signup",
      "dest": "/api/signup.js"
    },
    {
      "src": "/api/delete-appointment",
      "dest": "/api/delete-appointment.js"
    },
    {
      "src": "/api/mod-appointment",
      "dest": "/api/mod-appointment.js"
    },
    {
      "src": "/api/submit-appointment",
      "dest": "/api/submit-appointment.js"
    }
  ]
}
