{
  "version": 2,
  "name": "capstone-backend",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/get-appointment",
      "dest": "/api/get-appointment.js"
    },
    {
      "src": "/api/login",
      "dest": "/api/login.js"
    }
  ]
}
